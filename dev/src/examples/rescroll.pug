extends /_layout.pug

append vars
  -
    page = "/examples/rescroll.html"

block pageBody

append style
  link(rel="stylesheet", href="/examples/css/rescroll.css")

block script_body
  script(src="/examples/js/rescroll.js")

block pageBody
  div.pl-localNav
    a.pl-localNav_testLink(href="#") test
    ul.pl-localNav_list
      li.pl-localNav_item
        a(href="#section-A") section A
      li.pl-localNav_item
        a(href="#section-B") section B
      li.pl-localNav_item
        a(href="#section-C") section C
      li.pl-localNav_item
        a(href="#section-D") section D

  section.pl-section#section-A section A
  section.pl-section#section-B section B
    div.pl-section_core
      img(src="./img/large.png", alt="")
  section.pl-section#section-C section C
  section.pl-section#section-D section D
